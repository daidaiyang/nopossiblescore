<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nopossiblescore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TranslucentTheme">
        <activity android:name=".activities.main.MainActivity">
        </activity>

        <activity android:name=".activities.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.login.LoginActivity"/>
        <activity android:name=".activities.granarysearch.GranarysearchActivity"/>
        <activity android:name=".activities.message.MessageActivity"/>
        <activity android:name=".activities.mine.MineActivity"/>
        <activity android:name=".activities.set.SetActivity"/>
        <activity android:name=".activities.give.GiveActivity"/>
        <activity android:name=".activities.record.RecordActivity"/>
    </application>

</manifest>